<% provide(:title, 'Create a new category') %>
<h1>Create a new category'</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@categoryr) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>S

      <%= f.submit "Create new category", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>